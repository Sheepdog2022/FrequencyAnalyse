{
    "configurations": [
        {
            "name": "ARM",
            "intelliSenseMode": "gcc-x64",
            "compilerPath": "arm-none-eabi-g++",
            "includePath": [
                "${workspaceFolder}/Inc",
                "${workspaceFolder}/Drivers/STM32F7xx_HAL_Driver/Inc",
                "${workspaceFolder}/Drivers/STM32F7xx_HAL_Driver/Inc/Legacy",
                "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32F7xx/Include",
                "${workspaceFolder}/Drivers/CMSIS/Include",
                "${workspaceFolder}/Drivers/CMSIS/DSP/Include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "USE_HAL_DRIVER",
                "STM32F767xx",
                "ARM_MATH_CM7",
                "__FPU_PRESENT = 1"
            ],
            "cStandard": "c11",
            "cppStandard": "c++14",
            "forcedInclude": [
                "${workspaceFolder}/Drivers/STM32F7xx_HAL_Driver/Inc/stm32f7xx_hal.h"
            ]
        }
    ],
    "version": 4
}